{
  "name": "server",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "node --watch-path=./src --watch-path=./prompts --no-warnings --loader ts-node/esm --env-file=.env src/index.ts",
    "dev:development": "node --watch-path=./src --watch-path=./prompts --no-warnings --loader ts-node/esm --env-file=.env.development.local src/index.ts",
    "dev:staging": "node --watch-path=./src --watch-path=./prompts --no-warnings --loader ts-node/esm --env-file=.env.staging.local src/index.ts",
    "dev:production": "node --watch-path=./src --watch-path=./prompts --no-warnings --loader ts-node/esm --env-file=.env.production.local src/index.ts",
    "start": "node --env-file=.env dist/index.js",
    "start:staging": "node --env-file=.env.staging.local dist/index.js",
    "start:production": "node --env-file=.env.production.local dist/index.js",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "smoke": "node --loader ts-node/esm --env-file=.env tests/smoke.ts",
    "smoke:staging": "node --loader ts-node/esm --env-file=.env.staging.local tests/smoke.ts",
    "smoke:simple": "node tests/simple-smoke.mjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@supabase/supabase-js": "^2.80.0",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "openai": "^6.8.1",
    "puppeteer": "^24.30.0",
    "undici": "^7.16.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.10"
  }
}
